x=int(input("Enter any number"))
def happynumber(x):
    rem=0
    sum=0
    while(x>0):
        rem=x%10
        sum=sum+(rem*rem)
        x=x//10
        return sum
result=x
while(result!=1 and result!=4):
    result=happynumber(result)
if(result==1):
    print("Happy number")
if(result==4):
    print("not a happy number")

    
    
